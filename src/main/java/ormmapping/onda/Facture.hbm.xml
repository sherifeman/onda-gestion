<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onda.Facture" table="Facture" lazy="false">
		<id name="id_Fac" column="Id_Fac" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="devise" column="DeviseId_Devise" class="onda.Devise" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="op" cascade="lock" column="OrdrePaimentId_Op" class="onda.OrdrePaiment" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="fournisseur" cascade="lock" column="FournisseurId_Frs" class="onda.Fournisseur" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="doceng" column="Doceng" type="string" length="255" not-null="false" lazy="false"/>
		<property name="ref_Fac" column="Ref_Fac" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="date_Fac" column="Date_Fac" type="date" not-null="false" lazy="false"/>
		<property name="mt_Fac" column="Mt_Fac" type="double" length="10" not-null="false" lazy="false"/>
		<set name="ORM_Motifs" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="FactureId_Fac" not-null="true"/>
			<one-to-many class="onda.Motif"/>
		</set>
		<set name="ORM_Journals" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="FactureId_Fac" not-null="true"/>
			<one-to-many class="onda.Journal"/>
		</set>
		<set name="ORM_Historiques" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="FactureId_Fac" not-null="true"/>
			<one-to-many class="onda.HistoriqueActions"/>
		</set>
	</class>
</hibernate-mapping>
