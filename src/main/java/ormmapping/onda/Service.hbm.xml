<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onda.Service" table="Service" lazy="false">
		<id name="id_Serv" column="Id_Serv" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="libelle" column="Libelle" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Users" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="ServiceId_Serv" not-null="true"/>
			<one-to-many class="onda.User"/>
		</set>
	</class>
</hibernate-mapping>
