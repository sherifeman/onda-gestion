<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onda.OrdrePaiment" table="OrdrePaiment" lazy="false">
		<id name="id_Op" column="Id_Op" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="obj" column="Obj" type="string" length="255" not-null="false" lazy="false"/>
		<property name="date_Op" column="Date_Op" type="date" not-null="false" lazy="false"/>
		<property name="date_Bc" column="Date_Bc" type="date" not-null="false" lazy="false"/>
		<property name="date_SignT" column="Date_SignT" type="date" not-null="false" lazy="false"/>
		<property name="date_Dg" column="Date_Dg" type="date" not-null="false" lazy="false"/>
		<property name="num_Paie" column="Num_Paie" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="nom_Benf" column="Nom_Benf" type="string" length="255" not-null="false" lazy="false"/>
		<property name="deduc" column="Deduc" type="double" length="10" not-null="false" lazy="false"/>
		<property name="mt_Total" column="Mt_Total" type="double" length="10" not-null="false" lazy="false"/>
		<property name="mt_Op" column="Mt_Op" type="double" length="10" not-null="false" lazy="false"/>
		<one-to-one name="rejet" class="onda.Rejet" cascade="save-update,lock" property-ref="op" lazy="no-proxy" access="field"/>
		<set name="ORM_Factures" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="OrdrePaimentId_Op" not-null="true"/>
			<one-to-many class="onda.Facture"/>
		</set>
	</class>
</hibernate-mapping>
