<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onda.Profil" table="Profil" lazy="false">
		<id name="id_Profil" column="Id_Profil" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nom_Profil" column="Nom_Profil" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Fonctions" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="ProfilId_Profil" not-null="false"/>
			<one-to-many class="onda.Fonction"/>
		</set>
	</class>
</hibernate-mapping>
