<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onda.Motif" table="Motif" lazy="false">
		<id name="id_Motif" column="Id_Motif" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="rejet" cascade="lock" column="RejetId_Rejet" class="onda.Rejet" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="facture" cascade="lock" column="FactureId_Fac" class="onda.Facture" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="nom_Motif" column="Nom_Motif" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
