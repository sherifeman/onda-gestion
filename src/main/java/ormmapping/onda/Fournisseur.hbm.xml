<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onda.Fournisseur" table="Fournisseur" lazy="false">
		<id name="id_Frs" column="Id_Frs" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nom_Frs" column="Nom_Frs" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Factures" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="FournisseurId_Frs" not-null="true"/>
			<one-to-many class="onda.Facture"/>
		</set>
	</class>
</hibernate-mapping>
